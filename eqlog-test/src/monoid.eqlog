type M;
// The neutral element.
func e() -> M;
// The multiplication operation.
func mul(M, M) -> M;

rule {
    then e()!;
}
rule {
    if x: M;
    if y: M;
    then mul(x, y)!;
}
rule {
    if y = mul(x, e());
    then x = y;
}
rule {
    if y = mul(e(), x);
    then x = y;
}
rule {
    if d = mul(mul(a, b), c);
    if e = mul(a, mul(b, c));
    then d = e;
}
